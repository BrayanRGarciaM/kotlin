<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/product_graph"
    app:startDestination="@id/productFragment">

    <fragment
        android:id="@+id/productFragment"
        android:name="e.bolsadeideas.fanatic.ui.ProductFragment"
        android:label="fragment_product"
        tools:layout="@layout/fragment_product" >
        <action
            android:id="@+id/action_productFragment_to_cardFragment"
            app:destination="@id/cardFragment" />
    </fragment>
    <fragment
        android:id="@+id/cardFragment"
        android:name="e.bolsadeideas.fanatic.ui.CardFragment"
        android:label="fragment_card"
        tools:layout="@layout/fragment_card">
        <deepLink
            android:id="@+id/deepLink"
            app:uri="fanatic://card" />
        <argument
            android:name="nombre"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="NONE_NAME" />
    </fragment>
</navigation>